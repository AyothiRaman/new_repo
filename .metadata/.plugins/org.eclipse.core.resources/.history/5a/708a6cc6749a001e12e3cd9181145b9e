class Person 
//implements Comparable<Person>
{
	
	int age ;
	String name ;
	int id;
	
	public Person(int age, String name,int id) {
		
		this.age = age;
		this.name = name;
		this.id=id;
	}

	@Override
	public String toString() {
		return "Person [age=" + age + ", name=" + name + ", id=" + id + "]";
	}
	


