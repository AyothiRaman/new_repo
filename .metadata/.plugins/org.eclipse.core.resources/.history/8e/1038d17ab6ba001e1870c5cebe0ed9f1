package com.basicDemo.controller;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

import com.basicDemo.entity.Student;

public class ContClass {
	
	public void data() {
		
		List<String > hh = Arrays.asList("jdnd");
		
		hh.forEach(s->{
			System.out.println(s.contains("kk"));
		});
		
		
		
		List<Student > list = new ArrayList<>();
		list.add(new Student("sara", 90, 330));
		list.add(new Student("mara", 90, 330));

		list.add(new Student("bora", 90, 330));
		
		for(int i=0;i<list.size();i++) {
			System.out.println(list.get(i));
		}
		
		

		
	}
	

}
